
var i=0;
while (i<=50) {
    
    if (i%7==0)
        console.log(i+"-> boom");
    i++;
}

var i=0;
while (i<=100) {
    
    if (i%3==0) {
        if (i%5==0)
            console.log(i+" fizzbuzz");
        console.log(i+" fizz");        
    }
    if (i%5==0) 
        console.log(i+" buzz");   
        
    i++;
}
